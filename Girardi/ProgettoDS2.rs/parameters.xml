<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="post_probability" displayName="Post probability" type="double" 
						defaultValue="0.5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="size" displayName="Grid Size" type="int" 
						defaultValue="30" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="relay_count" displayName="Relay Count" type="int" 
						defaultValue="125" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="relay_range" displayName="Relay Range" type="double" 
						defaultValue="5.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="prob_msg_lost" displayName="Probability Message Lost" type="double" 
						defaultValue="0.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="trust_probability" displayName="Trust probability" type="double" 
						defaultValue="0.1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="clump_count" displayName="Cluster count" type="int" 
						defaultValue="5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="map_kind" displayName="Map kind" type="java.lang.String" 
						defaultValue="uniform" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="uniform cluster "
						
		/>
		</parameters>


